{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": ["react", "jest", "prettier"],
  "extends": ["prettier", "prettier/react", "plugin:react/recommended"],
  "rules": {
    "semi": [2, "never"]
  }
}
